.m-trip{
  overflow: hidden;
  position: relative;
  border-radius: 8rpx;
  margin-bottom: 20rpx;
}
.m-trip .cover{
  width: 100%;
  height: 352rpx;
  vertical-align: bottom;
}
.m-trip::before{
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  width: 560rpx;
  height: 100%;
  border-top-left-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
  background: linear-gradient(to right,rgba(0,0,0,.6) 0,rgba(0,0,0,0) 100%);
}
.m-trip .info{
  color: #fff;
  padding: 0 30rpx;
  position: absolute;
  top:0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: 1;
  box-sizing: border-box;
}
.m-trip .info .h2{
  font-size: 36rpx;
  font-weight: 600;
  line-height: 1.22;
  text-shadow: 2rpx 2rpx rgba(0,0,0,.4);
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  margin-top: 30rpx;
}
.m-trip .detail{
  margin-top: 10rpx;
  padding-left: 18rpx;
  position: relative;
  font-size: 18rpx;
  line-height: 1;
  font-weight: bold;
  text-shadow: 2rpx 4rpx 0px rgba(0,0,0,0.20);
}
.m-trip .detail view{
  margin-bottom: 6rpx;
}
.m-trip .detail text{
  margin-right: 20rpx;
}
.m-trip .detail .place{
  font-weight: normal;
  font-size: 16rpx;
}
.m-trip .detail::before{
  content: '';
  width: 6rpx;
  height: 100%;
  border-right: 1rpx;
  background-color: #4ABDCC;
  position: absolute;
  left: 4rpx;
  top: 2rpx;
}
.m-trip .user{
  position: absolute;
  width: 100%;
  left: 30rpx;
  bottom: 30rpx;
  line-height: 50rpx;
  font-size: 18rpx;
}

.m-trip .bottom{
  position: absolute;
  bottom: 24rpx;
  font-size: 20rpx;
}


.m-trip{
  margin: 20rpx;
}
.trip-title{
  margin-bottom: 10rpx;
}
.loading{
  color: #b0b0b0;
  text-align: center;
  margin: 20rpx 0;
}
.loading-gif{
  width: 220rpx;
  height: 42rpx;
  vertical-align: bottom;
}