/**列表**/
.ibox-list{  
    
    padding: 30rpx;     
}
.ibox-list-nav{ 
    background: #fff;
    position: relative
}
.ibox-list-nav view::after{
    clear:both;
    content:'.';
    display:block;
    width: 0;
    height: 0;
    visibility:hidden;
}
.ibox-list-img{
    width: 210rpx;
    height: 280rpx;
    float: left;
}
.ibox-list-infos{
    width: 450rpx;  
    float: right;
}
.ibox-list-author{
    font-size: 28rpx;
    color: #656565;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    line-height: 38rpx;
    margin-bottom: 6rpx;
}
.ibox-list-title{
    font-size: 34rpx;
    color: #2d2d2d;
    display: block;
    text-overflow: ellipsis;
    margin-top: 4rpx;
}
.ibox-list-star{
    margin:12rpx 0 14rpx;
}

.ibox-list-tags text{
    position: relative;
    font-size: 24rpx;
    color: #ffac2d;
    padding: 12rpx 16rpx;
    display: inline-block;
    margin-right: 14rpx;
    margin-bottom: 10rpx;

}

.ibox-list-tags text::after {
    content:" ";
    width:200%;
    height:200%;
    position:absolute;
    top:0;
    left:0;
    border:1px solid #ffac2d;
    -webkit-transform:scale(0.5);
    transform:scale(0.5);
    -webkit-transform-origin:0 0;
    transform-origin:0 0;
    box-sizing:border-box;
    border-radius:6px;

}

.ibox-list-summary{
    font-size: 28rpx;
    color: #656565;
    word-break:break-all;
    display:-webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
    overflow:hidden;
    line-height: 38rpx
}
.widget-line {
    content:" ";
    position:absolute;
    left:30rpx;
    top:0;
    right:30rpx;
    height:1px; 
    background-color:#F0F0F0;
    -webkit-transform:scaleY(0.5);
    transform:scaleY(0.5);

}
/**详情**/

.detail-box  {
    position: relative;
    width: 100%;
    height: 600rpx;
    display: flex;
    justify-content: center;
    align-content: center;
    overflow: hidden;
}
.detail-box:before {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    z-index: -1;
    width: 100%;
    height: 600rpx;
    background-color: rgba(45, 45, 45, .3);
}
.detail-box-bg {
    position: absolute;
    z-index: -2;
    left: 0;
    top: 0;
    width: 100%;
    height: 600rpx;
    background-size: cover;
    background-position: center;
    filter: blur(30rpx);
    transform: scale(1.2);
}

.detail-img{
    z-index: 9999;
    width: 354rpx;
    height: 504rpx;
    margin-top: 47rpx;
    box-shadow: 0 0rpx 30rpx rgba(0, 0, 0, .3)
}
.detail-info{
    background: #fff;
    padding: 50rpx 30rpx 30rpx;
    display: flex;
    justify-content: space-between;
    margin-bottom: 30rpx;
}
.detail-fl{
    width:500rpx;
}
.detail-fr{
    width:170rpx;
}
.detail-title{
    font-size: 50rpx;
    color: #2d2d2d;
    font-weight: bold;
    margin-bottom: 20rpx;
}
.detail-text{
    font-size: 28rpx;
    color: #656565;
    margin-bottom: 14rpx;

}

.detail-tags text{
    position: relative;
    font-size: 28rpx;
    color: #ffac2d;
    padding: 14rpx 18rpx;
    display: inline-block;
    margin-right: 16rpx;
    margin-bottom: 14rpx;
    margin-top: 20rpx;

}
.detail-tags text::after {
    content:" ";
    width:200%;
    height:200%;
    position:absolute;
    top:0;
    left:0;
    border:1px solid #ffac2d;
    -webkit-transform:scale(0.5);
    transform:scale(0.5);
    -webkit-transform-origin:0 0;
    transform-origin:0 0;
    box-sizing:border-box;
    border-radius:6px;
}

.detail-star-box{
    width: 167rpx;
    height: 167rpx;
    box-shadow: 0 0rpx 20rpx rgba(0, 0, 0, .2);
    background: #fff;
    display: flex;
